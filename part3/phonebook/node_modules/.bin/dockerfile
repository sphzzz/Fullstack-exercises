#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/C:/Users/DELL/Documents/GitHub/Fullstack-exercises/part3/phonebook/node_modules/.store/@flydotio+dockerfile@0.5.8/node_modules/@flydotio"
else
  export NODE_PATH="$NODE_PATH:$basedir/C:/Users/DELL/Documents/GitHub/Fullstack-exercises/part3/phonebook/node_modules/.store/@flydotio+dockerfile@0.5.8/node_modules/@flydotio"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.store/@flydotio+dockerfile@0.5.8/node_modules/@flydotio/dockerfile/index.js" "$@"
else
  exec node  "$basedir/../.store/@flydotio+dockerfile@0.5.8/node_modules/@flydotio/dockerfile/index.js" "$@"
fi
